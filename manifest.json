{
    "manifest_version": 3,
    "name": "Group Tabs by Base URL",
    "description": "Groups all open tabs based on their base URL.",
    "version": "1.2",
    "permissions": ["tabs", "storage", "tabGroups"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "icon.jpg",
        "48": "icon.jpg",
        "128": "icon.jpg"
      },
      "default_title": "Group Tabs"
    },
    "icons": {
      "16": "icon.jpg",
      "48": "icon.jpg",
      "128": "icon.jpg"
    }
  }
  